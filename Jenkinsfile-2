pipeline {
  agent { label 'agent1' }

  environment{
    MY_TOKEN = credentials('52')
  }
  stages {
    stage('Copy cred data') {
      steps {
        sh '''
            echo $MY_TOKEN >> test.txt
        '''
      }
    }
    stage('Print'){
        steps {
            sh '''
            cat test.txt
            '''
        }
    }
  }
}
